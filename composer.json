{
    "name": "tacman/composer-json-manipulator",
    "description": "Package to load, merge and save composer.json file(s)",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "nette/utils": "^4.0",
        "symfony/config": "^6.3 | ^7.0",
        "symfony/dependency-injection": "^6.3 | ^7.0",
        "symfony/filesystem": "^6.3 | ^7.0",
        "symplify/package-builder": "*",
        "symplify/symplify-kernel": "*",
        "symplify/smart-file-system": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.26"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ComposerJsonManipulator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ComposerJsonManipulator\\Tests\\": "tests"
        }
    },
    "extra": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
